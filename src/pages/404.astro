---
import Layout from "../layouts/Layout.astro";
const canonicalURL = new URL(
    Astro.url.pathname,
    "https://unioncoruna.com"
).toString();

const socialImageURL = new URL(
    "/images/jAyckLebI-k.webp",
    "https://unioncoruna.com"
).toString();
const meta = {
    title: `Unión Coruña | Página no encontrada`,
    description: `Página no encontrada`,
};
//            src="https://images.unsplash.com/photo-1545972154-9bb223aac798?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=3050&q=80&exp=8&con=-15&sat=-75"
//                 https://images.unsplash.com/photo-1619779398329-af8d6af0b352?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=MnwxfDB8MXxyYW5kb218MHx8fHx8fHx8MTY4NjMxNjkyMQ&ixlib=rb-4.0.3&q=80&w=1080
//                 https://images.unsplash.com/photo-1499939555716-ec66bcc4562c?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=MnwxfDB8MXxyYW5kb218MHx8fHx8fHx8MTY4NjA3NzIxNA&ixlib=rb-4.0.3&q=80&w=1080
---

<Layout
    title="Unión Coruña"
    description="Grupo de empresas que se ayudan mutuamente"
    metas={[
        { property: "og:url", content: canonicalURL },
        { property: "og:type", content: "website" },
        { property: "og:title", content: meta.title },
        { property: "og:description", content: meta.description },
        { property: "og:image", content: socialImageURL },
        { name: "twitter:card", content: "summary_large_image" },
        { property: "twitter:domain", content: "unioncoruna.com" },
        { property: "twitter:url", content: canonicalURL },
        { name: "twitter:title", content: meta.title },
        { name: "twitter:description", content: meta.description },
        { name: "twitter:image", content: socialImageURL },
    ]}
>
    <main class="relative isolate min-h-full">
        <img
            src="https://images.unsplash.com/photo-1499939555716-ec66bcc4562c?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=3050&q=80&exp=-8&con=-15&sat=-75"
            alt=""
            class="absolute inset-0 -z-10 h-full w-full object-cover object-top"
        />
        <div class="mx-auto max-w-7xl px-6 py-32 text-center sm:py-40 lg:px-8">
            <p class="text-base font-semibold leading-8 text-white">404</p>
            <h1
                class="mt-4 text-3xl font-bold tracking-tight text-white sm:text-5xl"
            >
                Página no encontrada
            </h1>
            <p class="mt-4 text-base text-white/70 sm:mt-6">
                No hemos podido encontrar la página que buscabas.
            </p>
            <div class="mt-10 flex justify-center">
                <a href="/" class="text-sm font-semibold leading-7 text-white"
                    ><span aria-hidden="true">&larr;</span> Volver al inicio
                </a>
            </div>
        </div>
    </main>
</Layout>
